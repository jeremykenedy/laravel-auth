{
  "name": "jeremykenedy/laravel-auth",
  "type": "project",
  "description": "Laravel Authentication Application.",
  "keywords": [
    "framework",
    "laravel",
    "Authentication"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-intl": "*",
    "creativeorange/gravatar": "^1.0",
    "doctrine/dbal": "^3.6",
    "google/recaptcha": "^1.3",
    "guzzlehttp/guzzle": "^7.2",
    "intervention/image": "^2.7",
    "jeremykenedy/laravel-blocker": "dev-laravel12",
    "jeremykenedy/laravel-email-database-log": "dev-fix/phpunit-gh-action",
    "jeremykenedy/laravel-exception-notifier": "^4.0",
    "jeremykenedy/laravel-https": "^2.0",
    "jeremykenedy/laravel-logger": "^10.0",
    "jeremykenedy/laravel-phpinfo": "^1.3",
    "jeremykenedy/laravel-roles": "^11.5",
    "jeremykenedy/laravel2step": "dev-fix/laravel12",
    "jeremykenedy/uuid": "^3.1",
    "laravel/framework": "^12.0",
    "laravel/sanctum": "^4.2",
    "laravel/socialite": "^5.6",
    "laravel/tinker": "^2.8",
    "laravel/ui": "^4.2",
    "nesbot/carbon": "^3.8",
    "predis/predis": "^2.1",
    "pusher/pusher-php-server": "^7.2",
    "rap2hpoutre/laravel-log-viewer": "^2.3",
    "socialiteproviders/37signals": "^4.1",
    "socialiteproviders/instagram": "^5.0",
    "socialiteproviders/linkedin": "^4.2",
    "socialiteproviders/twitch": "^5.3",
    "socialiteproviders/youtube": "^4.1",
    "spatie/laravel-html": "^3.12"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "laravel/breeze": "^2.3",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.18",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^8.8",
    "phpunit/phpunit": "^12.0",
    "spatie/laravel-ignition": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "jeremykenedy\\LaravelRoles\\": "vendor/jeremykenedy/laravel-roles/src/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "10.x-dev"
    },
    "laravel": {
      "dont-discover": []
    },
    "providers": [
      "jeremykenedy\\laravel-roles\\RolesServiceProvider"
    ],
    "aliases": {
      "laravelroles": "jeremykenedy\\laravel-roles\\RolesFacade"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "../laravel2step",
      "options": { "symlink": true }
    },
    {
      "type": "path",
      "url": "../laravel-email-database-log",
      "options": { "symlink": true }
    },
    {
      "type": "path",
      "url": "../laravel-blocker",
      "options": { "symlink": true }
    }
  ]
}
